count = wire(0)

def increment():
    print("Incrementing count")
    $count += 1

---html---
<section>
    <h1>Counter</h1>
    <p>{count.value}</p>
    <input></input>
    <Counter />
    <button @click={increment}>Increment Function</button>
    <!-- <a role="button" href="/pricing">View pricing</a> -->
</section>